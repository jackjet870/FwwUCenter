@model IEnumerable<WorkflowEngine.wfStepPath>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FromStepCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ToStepCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.wfCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PathType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FromStepCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ToStepCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.wfCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PathType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
