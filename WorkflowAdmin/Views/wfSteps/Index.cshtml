@model IEnumerable<WorkflowEngine.wfSteps>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StepCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StepName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Flag)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.wfCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsForcedApproved)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StepCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StepName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Flag)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.wfCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsForcedApproved)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StepCode }) |
            @Html.ActionLink("Details", "Details", new { id=item.StepCode }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StepCode })
        </td>
    </tr>
}

</table>
